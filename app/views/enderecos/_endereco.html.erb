<div id="<%= dom_id endereco %>">
  <p>
    <strong>Descricao:</strong>
    <%= endereco.descricao %>
  </p>

  <p>
    <strong>Rua:</strong>
    <%= endereco.rua %>
  </p>

  <p>
    <strong>Cidade:</strong>
    <%= endereco.cidade %>
  </p>

  <p>
    <strong>Estado:</strong>
    <%= endereco.estado %>
  </p>

  <p>
    <strong>Cep:</strong>
    <%= endereco.cep %>
  </p>

  <p>
    <strong>Cliente:</strong>
    <%= endereco.cliente_id %>
  </p>

  <p>
    <strong>Funcionario:</strong>
    <%= endereco.funcionario_id %>
  </p>

  <p>
    <strong>Endereco principal:</strong>
    <%= endereco.endereco_principal %>
  </p>

</div>
